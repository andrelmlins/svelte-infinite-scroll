{
  "name": "svelte-infinite-scroll",
  "version": "2.0.1",
  "description": "Infinite Scroll Component to Svelte",
  "repository": "https://github.com/andrelmlins/svelte-infinite-scroll",
  "author": "Andr√© Lins <andrelucas01@hotmail.com> (https://andrelmlins.github.io/)",
  "license": "MIT",
  "svelte": "dist/InfiniteScroll.svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://svelte-infinite-scroll.netlify.com/",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^2.0.1",
    "babel-jest": "^26.3.0",
    "jest": "^26.4.2",
    "rollup": "^2.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^0.4.4",
    "svelte": "^3.32.0",
    "svelte-dts": "^0.3.7",
    "svelte-jester": "1.1.5",
    "svelte-preprocess": "^4.7.4",
    "svelte-transpile-typescript": "^0.1.2",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "build": "rollup -c  && svelte-transpile-typescript -i src/lib/InfiniteScroll.svelte -o dist/InfiniteScroll.svelte",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest src/lib",
    "coverage": "npm run test --coverage"
  },
  "bugs": {
    "url": "https://github.com/andrelmlins/svelte-infinite-scroll/issues"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "keywords": [
    "svelte",
    "infinite",
    "scroll"
  ]
}
